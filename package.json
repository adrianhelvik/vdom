{
  "name": "@adrianhelvik/vdom",
  "version": "1.0.5",
  "main": "dist/index.js",
  "module": "src/index.js",
  "author": "Adrian Helvik <adrian.helvik@specifique.no>",
  "license": "MIT",
  "devDependencies": {
    "@adrianhelvik/mock": "^3.3.2",
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "coveralls": "^3.0.7",
    "jest": "^24.9.0"
  },
  "scripts": {
    "start": "jest --watch",
    "test": "jest --coverage",
    "build": "rm -r dist; babel src --out-dir dist --ignore '**/*.spec.js'",
    "prepublish": "npm run test && npm run build",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "pragma": "createElement",
          "pragmaFrag": "createElement.Fragment"
        }
      ]
    ],
    "plugins": ["@babel/plugin-proposal-class-properties"]
  },
  "dependencies": {
    "@adrianhelvik/fragdom": "^2.0.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5"
  }
}
